{
    "name": "beastbytes/token-php",
    "description": "Yii Token PHP storage",
    "keywords": [
        "php",
        "token",
        "yii3"
    ],
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.1",
        "beastbytes/token": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "BeastBytes\\Token\\Php\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeastBytes\\Token\\Php\\Tests\\": "tests/"
        }
    },
    "extra": {
        "config-plugin-options": {
            "source-directory": "config"
        },
        "config-plugin": {
            "di": "di.php"
        }
    },
    "suggest": {
        "beastbytes/token-uuid4": "Factory for UUID4 tokens"
    }
}